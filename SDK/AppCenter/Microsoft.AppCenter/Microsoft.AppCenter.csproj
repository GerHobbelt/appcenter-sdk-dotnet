    <FileVersion>3.0.1.0</FileVersion>
    <AssemblyTitle>Microsoft.AppCenter.Core</AssemblyTitle>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\Microsoft.AppCenter.xml</DocumentationFile>
